{
  "name": "angular-slicer",
  "version": "0.0.0",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron:start": "ng build --base-href ./ && electron .",
    "electron:build": "ng build --base-href ./ && npx electron-builder@24.13.3"
  },
  "build": {
    "appId": "com.example.angular-slicer",
    "productName": "AngularSlicer",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/angular-slicer/browser/**/*",
      "electron.js"
    ],
    "win": {
      "target": "nsis",
      "verifyUpdateCodeSignature": false
    },
    "mac": {
      "target": "dmg"
    }
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "js-clipper": "^1.0.1",
    "rxjs": "~7.8.0",
    "three": "^0.179.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/three": "^0.179.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "electron": "28.2.4",
    "electron-builder": "24.13.3"
  }
}
